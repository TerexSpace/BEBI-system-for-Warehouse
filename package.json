{
  "name": "blockchaindt",
  "version": "1.0.0",
  "description": "Research framework for distributed consensus in physical-digital state synchronization",
  "main": "erp-prototype/backend/app.js",
  "scripts": {
    "start": "cd erp-prototype/backend && node app.js",
    "dev": "cd erp-prototype/backend && nodemon app.js",
    "test": "cd erp-prototype && jest --runInBand --coverage",
    "fabric-stub": "cd erp-prototype/backend && node fabric-stub.js",
    "setup": "cd erp-prototype && pip install -r ../requirements.txt && npm install",
    "train-model": "cd erp-prototype && python optimization/train.py",
    "demo": "cd erp-prototype && python demo/demo_scenarios.py"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "log4js": "^6.9.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "blockchain",
    "distributed-systems",
    "digital-twin",
    "consensus-protocols",
    "byzantine-fault-tolerance",
    "machine-learning",
    "research-software",
    "hyperledger-fabric",
    "state-synchronization",
    "reproducible-research"
  ],
  "author": "Almas Ospanov <ospanov@astanait.edu.kz>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TerexSpace/BEBI-system-for-Warehouse.git"
  },
  "bugs": {
    "url": "https://github.com/TerexSpace/BEBI-system-for-Warehouse/issues"
  },
  "homepage": "https://github.com/TerexSpace/BEBI-system-for-Warehouse#readme"
}
